{
  "builds": [
    {
      "src": "api/autofill.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "30mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/autofill",
      "dest": "api/autofill.py"
    },
    {
      "src": "/api/cron/load-db",
      "dest": "api/autofill.py"
    },
    {
      "src": "/",
      "dest": "api/autofill.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/load-db",
      "schedule": "0 5 * * *"
    }
  ]
}